{
  "name": "ng2-fundamentals-demo",
  "version": "1.0.0",
  "engines": {
    "node": "5.9.1"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run server\" ",
    "server": "node ./server/server.js",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "bundle": "node server/bundler.js",
    "bundle:prod": "node server/bundler.js --prod",
    "lite-server-test": "lite-server --config=liteserver-test-config.json",
    "test": "karma start karma.conf.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12",
    "body-parser": "1.15.0",
    "cookie-parser": "1.4.1",
    "debug": "2.2.0",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-session": "1.13.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "morgan": "1.7.0",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.4",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "lite-server": "^2.2.2",
    "remap-istanbul": "^0.6.4",
    "systemjs-builder": "^0.15.19",
    "typescript": "^1.8.10",
    "typings": "^1.0.5"
  }
